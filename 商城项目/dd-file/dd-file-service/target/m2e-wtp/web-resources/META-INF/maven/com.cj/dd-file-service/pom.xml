			<groupId>com.cj</groupId>
			<artifactId>dd-file-interface</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
	</dependencies>

	<!-- 配置tomcat插件 -->
	<build>

		<plugins>

			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<configuration>
					<port>8091</port>
					<path>/ddshop</path>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>